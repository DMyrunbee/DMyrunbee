package sai;

public class ArrayList {

	public  static  Integer getsum(int... inte){
	    int sum = 0;
	    for(int i : inte) { 
	        sum += i;
	    }
	    return sum;
	}
	public static void main(String[] args) {
		
		
		System.out.println(s2.getsum(1));
		System.out.println(s2.getsum(1,3,5));
		System.out.println(s2.getsum(2,4,6));
	
	}

}